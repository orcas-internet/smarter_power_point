{
    "name": "orcas Smarter Power Point",
    "version": "1.1.0",
    "manifest_version": 2,
    "options_ui": {
        "page": "html/admin.html",
        "open_in_tab": true
    },
    "browser_action": {
        "default_icon": {
            "16": "img/kitchen-16.png"
        },
        "default_popup": "html/options.html"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "permissions": [
        "notifications",
        "alarms",
        "storage",
        "http://*/",
        "activeTab",
        "https://ajax.googleapis.com/"
    ],
    "icons": {
        "16": "img/kitchen-16.png",
        "128": "img/kitchen-128.png"
    }
}
